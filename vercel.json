{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/evio-modern-hr-form.html" },
    { "src": "/index.html", "dest": "/evio-modern-hr-form.html" },
    { "src": "/form", "dest": "/evio-modern-hr-form.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
